{
    "name": "laravel/code-cleaner",
    "description": "Laravel package to remove dump, dd, console statements from code - Compatible with Laravel 7-12+",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3",
        "illuminate/console": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "symfony/finder": "^4.0|^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "CodeCleaner\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeCleaner\\Providers\\CodeCleanerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}